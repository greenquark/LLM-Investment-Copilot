# Secrets Configuration Template
# Copy this file to secrets.yaml and fill in your actual API tokens.
# DO NOT commit secrets.yaml to version control - it is gitignored.

# Data Sources API Tokens
data_sources:
  marketdata_app:
    api_token: "YOUR_MARKETDATA_APP_API_TOKEN"  # Get from https://marketdata.app

# Strategy-specific API Keys
strategies:
  llm_trend_detection:
    openai_api_key: "YOUR_OPENAI_API_KEY"  # Get from https://platform.openai.com/api-keys

# Moomoo Configuration (for live trading)
moomoo:
  account_id: "YOUR_ACCOUNT_ID"  # Replace with your actual account ID

